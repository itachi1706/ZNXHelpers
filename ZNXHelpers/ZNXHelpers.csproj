<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <ReleaseVersion>0.2</ReleaseVersion>
    <PackOnBuild>true</PackOnBuild>
    <PackageVersion>1.2.0</PackageVersion>
    <Configurations>Release;Debug</Configurations>
    <Authors>Kenneth Soh</Authors>
    <NeutralLanguage>en-SG</NeutralLanguage>
    <PackageReleaseNotes>First Release</PackageReleaseNotes>
    <Summary>Helper Class for ZNX apps</Summary>
    <Title>ZNX Helpers</Title>
    <Description>Common Helper class for various common functions</Description>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="AWSSDK.S3" />
    <None Remove="AWSSDK.SimpleSystemsManagement" />
    <None Remove="AWSSDK.SecretsManager" />
    <None Remove="AWSSDK.KeyManagementService" />
    <None Remove="Serilog" />
    <None Remove="Newtonsoft.Json" />
    <None Remove="System.IdentityModel.Tokens.Jwt" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.7.2.5" />
    <PackageReference Include="AWSSDK.SimpleSystemsManagement" Version="3.7.5.11" />
    <PackageReference Include="AWSSDK.SecretsManager" Version="3.7.1.18" />
    <PackageReference Include="AWSSDK.KeyManagementService" Version="3.7.2.9" />
    <PackageReference Include="Serilog" Version="2.10.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.12.2" />
  </ItemGroup>
</Project>
