<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ReleaseVersion>0.2</ReleaseVersion>
    <PackOnBuild>true</PackOnBuild>
    <PackageVersion>2.0.1</PackageVersion>
    <Configurations>Release;Debug</Configurations>
    <Authors>Zionext</Authors>
    <NeutralLanguage>en-SG</NeutralLanguage>
    <PackageReleaseNotes>First .NET 6 Release
- Fixed GetString nullability checks</PackageReleaseNotes>
    <Summary>Helper Class for ZNX apps</Summary>
    <Title>ZNX Helpers</Title>
    <Description>Common Helper class for various common functions</Description>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <PackageProjectUrl>https://github.com/itachi1706/ZNXHelpers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/itachi1706/ZNXHelpers.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Copyright>2022 Zionext Pte Ltd</Copyright>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="AWSSDK.S3" />
    <None Remove="AWSSDK.SimpleSystemsManagement" />
    <None Remove="AWSSDK.SecretsManager" />
    <None Remove="AWSSDK.KeyManagementService" />
    <None Remove="Serilog" />
    <None Remove="Newtonsoft.Json" />
    <None Remove="System.IdentityModel.Tokens.Jwt" />
    <None Remove="AWSSDK.Core" />
    <None Remove="AWSSDK.SecurityToken" />
    <None Remove="Exceptions\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="3.7.101.17" />
    <PackageReference Include="AWSSDK.SimpleSystemsManagement" Version="3.7.101.7" />
    <PackageReference Include="AWSSDK.SecretsManager" Version="3.7.100.17" />
    <PackageReference Include="AWSSDK.KeyManagementService" Version="3.7.100.17" />
    <PackageReference Include="AWSSDK.Core" Version="3.7.100.17" />
    <PackageReference Include="AWSSDK.SecurityToken" Version="3.7.100.21" />
    <PackageReference Include="Serilog" Version="2.12.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="6.25.0" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Exceptions\" />
  </ItemGroup>
</Project>
